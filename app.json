{
  "expo": {
    "name": "Circle",
    "slug": "Circle",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "circle",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "37",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsInWebContent": true
        },
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Circle uses your location to find nearby matches and improve your dating experience, even when the app is closed.",
        "NSLocationWhenInUseUsageDescription": "Circle uses your location to find nearby matches and improve your dating experience.",
        "NSPhotoLibraryUsageDescription": "Circle needs access to your photo library to save images and videos from chat messages.",
        "UIBackgroundModes": [
          "location",
          "location"
        ]
      },
      "bundleIdentifier": "com.orincore.Circle",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#7C2B86",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "com.android.vending.BILLING",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "versionCode": 37,
      "package": "com.orincore.Circle",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.orincore.Circle",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "name": "Circle",
      "shortName": "Circle",
      "description": "No Swiping Dating & Friendship App with AI-Powered Smart Matching",
      "themeColor": "#7C2B86",
      "backgroundColor": "#7C2B86",
      "lang": "en"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Circle uses your location to find nearby matches and improve your dating experience, even when the app is closed.",
          "locationWhenInUsePermission": "Circle uses your location to find nearby matches and improve your dating experience."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#7C2B86",
          "defaultChannel": "default",
          "sounds": []
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          },
          "ios": {
            "googleServicesFile": "./GoogleService-Info.plist"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#7C2B86",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#1F1147"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Circle needs access to your photo library to save images and videos from chat messages.",
          "savePhotosPermission": "Circle needs permission to save images and videos to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-7904629558122562~8832481283",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c9234d97-8ff5-45bc-8f8f-8772ef0926a5"
      }
    },
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-7904629558122562~8832481283",
      "ios_app_id": "ca-app-pub-3940256099942544~1458002511"
    }
  }
}
